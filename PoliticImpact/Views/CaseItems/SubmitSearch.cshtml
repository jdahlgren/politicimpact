

@{
    ViewBag.Title = "SubmitSearch";
}



<ul data-role="listview">
@foreach (var item in ViewBag.Result) {
    <li>
        <a href= "/CaseItems/Details/@item.ID" data-transition="slide" rel="external"> 
            <h3>@item.Title</h3>
            <p><strong>Anders Andersson</strong></p>
            <p>@item.Text.</p>
            <p class="ui-li-aside">4 Likes</p>
        </a>
     </li>
}
</ul>
